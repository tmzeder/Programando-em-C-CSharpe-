using System;
					
public class Program
{ //Sintaxe do FOR 
	public static void Main()
	{ 
		string[] palavras = {"Exemplo_1", "Exemplo_2", "Exemplo_3", "Exemplo_4", "Exemplo_5"};
		
		
		//Lembrando que nosso array/vetor tem 5 elementos e o contador não deve ser maior que o array
		
		for(int contador = 4; contador >= 0 ;contador--){
			//Enquanto o contador for maior ou igual a zero, subtraimos 1
			//Lembrando que o array começa a contar do zero e não do 1
			
			Console.WriteLine(palavras[contador] +" Rodando For"); //concatenando a variavel junto com string
			Console.WriteLine(contador);
			Console.WriteLine("Rodando FOR na linha abaixo. "); // forma tradicional.
			
		}
		Console.WriteLine("Fim do Programa" );
		Console.ReadLine();
	}
}
